        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@tokens-studio/sd-transforms": "^1.3.0",
        "@types/react-dom": "^19.1.4",
        "@vitejs/plugin-react": "^4.0.0",
        "ajv": "^8.17.1",
        "chokidar": "^4.0.3",
        "fast-deep-equal": "^3.1.3",
        "nodemon": "^3.1.10",
        "style-dictionary": "^3.8.0",
        "typescript": "^5.0.0",
        "vite": "^4.0.0"
