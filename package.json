{
    "name": "ai-design-system",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "build:tokens": "style-dictionary build --config config.mjs",
