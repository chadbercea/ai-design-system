{
  "name": "ai-design-system",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.0.10",
    "@storybook/addon-onboarding": "^9.0.10",
    "@storybook/builder-webpack5": "^9.0.10",
    "@storybook/preset-create-react-app": "^9.0.10",
    "@storybook/react": "^9.0.10",
    "@storybook/react-webpack5": "^9.0.10",
    "@tokens-studio/sd-transforms": "^2.0.0",
    "eslint-plugin-storybook": "^9.0.10",
    "prop-types": "^15.8.1",
    "react-docgen-typescript": "^2.4.0",
    "storybook": "^9.0.10",
    "style-dictionary": "^5.0.0",
    "ts-loader": "^9.5.2",
    "webpack": "^5.99.9"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:tokens": "style-dictionary build --config config/style-dictionary.config.mjs && node scripts/generate-shadcn-fonts.js && node build/mui/validate-theme-mapping.js",
    "watch:tokens": "style-dictionary build --config config/style-dictionary.config.mjs --watch",
    "validate:theme": "node build/mui/validate-theme-mapping.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
