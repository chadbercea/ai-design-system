{
  "dimension": {
    "scale": {
      "$type": "dimension",
      "$value": "2",
      "$description": "Base scale factor"
    },
    "xs": {
      "$type": "dimension",
      "$value": "4",
      "$description": "Extra small dimension"
    },
    "sm": {
      "$type": "dimension",
      "$value": "{dimension.xs} * {dimension.scale}",
      "$description": "Small dimension value"
    },
    "md": {
      "$type": "dimension",
      "$value": "{dimension.sm} * {dimension.scale}",
      "$description": "Medium dimension value"
    },
    "lg": {
      "$type": "dimension",
      "$value": "{dimension.md} * {dimension.scale}",
      "$description": "Large dimension value"
    },
    "xl": {
      "$type": "dimension",
      "$value": "{dimension.lg} * {dimension.scale}",
      "$description": "Extra large dimension"
    }
  },
  "spacing": {
    "xs": {
      "$type": "spacing",
      "$value": "{dimension.xs}",
      "$description": "Extra small spacing"
    },
    "sm": {
      "$type": "spacing",
      "$value": "{dimension.sm}",
      "$description": "Small spacing value"
    },
    "md": {
      "$type": "spacing",
      "$value": "{dimension.md}",
      "$description": "Medium spacing value"
    },
    "lg": {
      "$type": "spacing",
      "$value": "{dimension.lg}",
      "$description": "Large spacing value"
    },
    "xl": {
      "$type": "spacing",
      "$value": "{dimension.xl}",
      "$description": "Extra large spacing"
    },
    "multi-value": {
      "$type": "spacing",
      "$value": "{dimension.sm} {dimension.xl}",
      "$description": "You can have multiple values in a single spacing token. Read more on these: https://docs.tokens.studio/available-tokens/spacing-tokens#multi-value-spacing-tokens"
    }
  },
  "borderRadius": {
    "sm": {
