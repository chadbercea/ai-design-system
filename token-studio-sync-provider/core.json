{
  "h1": { "$type": "fontSize", "$value": "roundTo({body}*1.25^5)", "$description": "Font size for h1" },
  "h2": { "$type": "fontSize", "$value": "roundTo({body}*1.25^4)", "$description": "Font size for h2" },
  "h3": { "$type": "fontSize", "$value": "roundTo({body}*1.25^3)", "$description": "Font size for h3" },
  "h4": { "$type": "fontSize", "$value": "roundTo({body}*1.25^2)", "$description": "Font size for h4" },
  "h5": { "$type": "fontSize", "$value": "roundTo({body}*1.25^1)", "$description": "Font size for h5" },
  "h6": { "$type": "fontSize", "$value": "{body}", "$description": "Font size for h6" },
  "body": { "$type": "fontSize", "$value": "16", "$description": "Body font size" },
  "sm": { "$type": "fontSize", "$value": "{body} * 0.85", "$description": "Small font size" },
  "xs": { "$type": "fontSize", "$value": "{body} * 0.65", "$description": "Extra small font size" },
  "scale": { "$type": "dimension", "$value": "2", "$description": "Base scale factor" },
  "xs": { "$type": "dimension", "$value": "4", "$description": "Extra small dimension" },
  "sm": { "$type": "dimension", "$value": "{xs} * {scale}", "$description": "Small dimension value" },
  "md": { "$type": "dimension", "$value": "{sm} * {scale}", "$description": "Medium dimension value" },
  "lg": { "$type": "dimension", "$value": "{md} * {scale}", "$description": "Large dimension value" },
  "xl": { "$type": "dimension", "$value": "{lg} * {scale}", "$description": "Extra large dimension" },
  "xsSpacing": { "$type": "spacing", "$value": "{xs}", "$description": "Extra small spacing" },
  "smSpacing": { "$type": "spacing", "$value": "{sm}", "$description": "Small spacing value" },
  "mdSpacing": { "$type": "spacing", "$value": "{md}", "$description": "Medium spacing value" },
  "lgSpacing": { "$type": "spacing", "$value": "{lg}", "$description": "Large spacing value" },
  "xlSpacing": { "$type": "spacing", "$value": "{xl}", "$description": "Extra large spacing" },
  "multiValueSpacing": { "$type": "spacing", "$value": "{sm} {xl}", "$description": "Multiple values in a single spacing token." },
  "opacityLow": { "$type": "opacity", "$value": "10%", "$description": "Low opacity value" },
  "opacityMedium": { "$type": "opacity", "$value": "50%", "$description": "Medium opacity value" },
  "opacityHigh": { "$type": "opacity", "$value": "90%", "$description": "High opacity value" },
  "headingRegular": { "$type": "fontWeight", "$value": "Regular", "$description": "Regular heading font weight" },
  "headingBold": { "$type": "fontWeight", "$value": "Bold", "$description": "Bold heading font weight" },
  "bodyRegular": { "$type": "fontWeight", "$value": "Regular", "$description": "Regular body font weight" },
  "bodyBold": { "$type": "fontWeight", "$value": "Bold", "$description": "Bold body font weight" },
  "heading": { "$type": "fontFamily", "$value": "Inter", "$description": "Heading font family" },
  "body": { "$type": "fontFamily", "$value": "Roboto", "$description": "Body font family" },
  "headingLineHeight": { "$type": "lineHeight", "$value": "110%", "$description": "Heading line height" },
  "bodyLineHeight": { "$type": "lineHeight", "$value": "140%", "$description": "Body line height" },
  "defaultLetterSpacing": { "$type": "letterSpacing", "$value": "0", "$description": "Default letter spacing" },
  "increasedLetterSpacing": { "$type": "letterSpacing", "$value": "150%", "$description": "Increased letter spacing" },
  "decreasedLetterSpacing": { "$type": "letterSpacing", "$value": "-5%", "$description": "Decreased letter spacing" },
  "h1ParagraphSpacing": { "$type": "paragraphSpacing", "$value": "32", "$description": "Paragraph spacing for h1" },
  "h2ParagraphSpacing": { "$type": "paragraphSpacing", "$value": "26", "$description": "Paragraph spacing for h2" },
  "smRadius": { "$type": "borderRadius", "$value": "4", "$description": "Small border radius" },
  "lgRadius": { "$type": "borderRadius", "$value": "8", "$description": "Large border radius" },
  "xlRadius": { "$type": "borderRadius", "$value": "16", "$description": "Extra large border radius" },
  "multiValueRadius": { "$type": "borderRadius", "$value": "{smRadius} {lgRadius}", "$description": "Multiple values in a single radius token." }
}